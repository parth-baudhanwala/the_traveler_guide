@model IEnumerable<RESULTportal.Models.Student>

@{
    ViewBag.Title = "viewstudent";
}

<h2></h2>

<p>
    @Html.ActionLink("Create New", "createstudent")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.semister)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Branch)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pointer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pssd)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.semister)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Branch)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pointer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pssd)
            </td>
            <td>

                @Html.ActionLink("Delete", "deletestudent", new { id = item.Id })
            </td>
        </tr>
    }
    @Html.ActionLink("go to index page", "index")

</table>
@Html.ActionLink("log out", "logout")